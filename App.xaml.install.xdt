<Application 
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ms-appx:///SlabResources/StandardLayout.xaml" xdt:Transform="Insert"/>
                <ResourceDictionary Source="ms-appx:///SlabResources/StandardStyles.xaml" xdt:Transform="Insert"/>
                <ResourceDictionary Source="ms-appx:///SlabResources/Converters.xaml" xdt:Transform="Insert"/>
                <ResourceDictionary Source="ms-appx:///SlabResources/Controls.xaml" xdt:Transform="Insert"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>